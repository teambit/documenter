{
  "bit": {
    "env": {},
    "extensions": {
      "flows": {
        "build": [
          "#@bit/bit.evangalist.extensions.react-ts:transpile"
        ],
        "start": [
          "build",
          "serve"
        ],
        "test": [],
        "e2e": []
      },
      "extensions/react-ts": {},
      "create": {
        "template": "extensions/react-ts"
      }
    },
    "componentsDefaultDirectory": "components/{name}",
    "packageManager": "npm",
    "defaultScope": "bit.evangalist",
    "overrides": {
      "extensions/*": {
        "extensions": {
          "flows": {
            "build": []
          }
        }
      }
    }
  },
  "dependencies": {
    "gulp": "^4.0.2",
    "gulp-typescript": "^6.0.0-alpha.1",
    "merge2": "^1.3.0",
    "react": "^16.12.0",
    "react-dom": "^16.13.1",
    "typescript": "^3.7.5"
  },
  "devDependencies": {
    "@types/react": "^16.9.17",
    "bit-bin": "latest"
  },
  "scripts": {
    "postinstall": "bit link"
  }
}
